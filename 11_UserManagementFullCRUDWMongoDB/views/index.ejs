<!-- Main Site-->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <a href="/add-user" class="border-shadow expand">
        <span class="text-gradient"
          >Novo Usuário <i class="fas fa-user"></i
        ></span>
      </a>
    </div>
    <!-- Form handling-->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>@Email</th>
            <th>Profissão</th>
            <th>Género</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%- include('include/_show') %>
        </tbody>
      </table>
    </form>
    <!-- /Form handling-->
  </div>
</main>
<!-- /Main Site-->

